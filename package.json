{
  "name": "myreact-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fespiritu/myreact-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Avengers",
  "license": "Apache-2.0",
  "homepage": "https://github.com/fespiritu/myreact-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.7.1",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.7.1"
  }
}
